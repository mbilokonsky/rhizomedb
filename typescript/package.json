{
  "name": "rhizomedb",
  "version": "0.1.0",
  "description": "A TypeScript reference implementation of the rhizomatic database.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbilokonsky/rhizomedb.git"
  },
  "keywords": [
    "database",
    "rhizome",
    "deltas",
    "crdts",
    "hypergraph",
    "crdt"
  ],
  "author": "mykola bilokonsky",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mbilokonsky/rhizomedb/issues"
  },
  "homepage": "https://github.com/mbilokonsky/rhizomedb#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/lru-cache": "^7.10.9",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "graphql": "^16.8.0",
    "level": "^8.0.0",
    "lru-cache": "^11.2.2",
    "uuid": "^9.0.0",
    "ws": "^8.18.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
